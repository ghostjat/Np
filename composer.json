{
    "name": "ghostjat/np",
    "description": "Np (numphp) is numerical library with low memory footprint for php language. Suitable for large scale computing. php-8.0+ 64 bit is required!",
    "type": "library",
    "keywords": [
        "arithmetic", "computation", "computing", "decomposition", "engineering",
        "linear algebra", "math", "matrix", "norms", "php","blas","lapack",
        "random number generation", "rng", "reduction", "scientific computing", "statistics",
        "tensor", "vector", "trigonometry","statstics","numphp","np"
    ],
    "homepage": "https://github.com/ghostjat/Np",
    "Docs" : "https://ghostjat.github.io/Np/",
    "license": "MIT",
    "authors": [
        {
            "name": "Shubham Chaudhary",
            "email": "ghost.jat@gmail.com",
            "role": "dev"
        }
    ],
    "support": {
        "issues": "https://github.com/ghostjat/numphp/issues?state=open",
        "source": "https://github.com/ghostjat/numphp",
        "email":"ghost.jat@gmail.com"
    },
    "autoload": {
        "psr-4": {
            "Np\\": "src/"
        }
    },
    "require": {
        "php-64bit": ">=8.0",
        "ext-ffi": "*"
    },
    
    "require-dev": {
        "phpunit/phpunit": "^9"
    },
    
    "autoload-dev": {
        "psr-4": {
            "Np\\tests\\": "tests/",
            "Np\\benchmarks\\": "benchmarks/"
        }
    },
    "config":{"optimize-autoloader":true}
}
